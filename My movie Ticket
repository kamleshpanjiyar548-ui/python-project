age = int(input("Enter your age :"))
ticket = True
if age >=20 :
    if ticket:
        print("you can watch the movie :")
    else:
        print("you have to bye ticket :")
elif age >=15 :
    print("you can watch movie with parents :")
    
elif age >=0:
    print("you are not eligible to watch movie :")
else :
    print("you baby not bron")
